<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace = "com.Board.BoardAPI.mapper.BoardUserMapper">
    <insert id="createUser" parameterType="com.Board.BoardAPI.domain.User">
        INSERT INTO tbl_user (userId, userPassword)
        VALUES
        (#{userId}, #{userPassword})
    </insert>

    <select id="getUser" parameterType="String" resultType="com.Board.BoardAPI.domain.User">
        SELECT
        *
        FROM tbl_user
        WHERE userId=#{userId}
    </select>
</mapper>